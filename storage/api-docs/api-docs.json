{
    "openapi": "3.0.0",
    "info": {
        "title": "SONIC API",
        "description": "SONIC API",
        "version": "3.x"
    },
    "paths": {
        "/": {},
        "/api/dang-nhap": {
            "post": {
                "tags": [
                    "NguoiDung"
                ],
                "summary": "Login",
                "operationId": "dangNhap",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "tai_khoan": {
                                        "description": "Tài Khoản",
                                        "type": "string"
                                    },
                                    "mat_khau": {
                                        "description": "Mật Khẩu",
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "500": {
                        "description": "Error server"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/phim": {
            "get": {
                "tags": [
                    "Phim"
                ],
                "summary": "Danh Sách Phim",
                "operationId": "layDanhSach",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "loai_phim_id": {
                                        "description": "Thể loại",
                                        "type": "string"
                                    },
                                    "kieu_phim_id": {
                                        "description": "Kiểu phim",
                                        "type": "string"
                                    },
                                    "nam_san_xuat": {
                                        "description": "Năm sản xuất",
                                        "type": "string"
                                    },
                                    "quoc_gia_id": {
                                        "description": "Quốc gia",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "500": {
                        "description": "Error server"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/tim-kiem": {
            "get": {
                "tags": [
                    "Tìm kiếm"
                ],
                "summary": "Tìm kiếm theo nhu cầu",
                "operationId": "TiemKiem",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "key_word": {
                                        "description": "Tìm kiếm",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "500": {
                        "description": "Error server"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/phim/{id}": {
            "get": {
                "tags": [
                    "Chi Tiết Trang"
                ],
                "summary": "Thông tin chi tiết trang",
                "operationId": "ChiTietTrang",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID phim",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64",
                            "minimum": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "500": {
                        "description": "Error server"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/phim/them-phim": {
            "post": {
                "tags": [
                    "Phim"
                ],
                "summary": "Cập nhật phim",
                "operationId": "TaoPhim",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "fileFilm": {
                                        "description": "Thêm file",
                                        "type": "file"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "500": {
                        "description": "Error server"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "Authorization",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}